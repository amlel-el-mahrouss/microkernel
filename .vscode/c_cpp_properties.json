{
  "configurations": [
    {
      "name": "kernel",
      "includePath": [
        "${workspaceFolder}/*",
        "${workspaceFolder}/fs/*",
        "${workspaceFolder}/libhart/*",
        "${workspaceFolder}/lib/*",
        "${workspaceFolder}/builtin/*",
        "${workspaceFolder}/libident/inc/*",
        "${workspaceFolder}/hal/riscv64/*",
        "${workspaceFolder}/hal/x86/*",
        "${workspaceFolder}/libkern/inc/*",
        "${workspaceFolder}/init/inc/*",
        "${workspaceFolder}/libmthread/*",
        "${workspaceFolder}/system_calls/*",
        "${workspaceFolder}/libmthread/inc/*"
      ],
      "defines": [
        "__KDEBUG__",
        "__KERNEL",
        "__unix__",
        "__KSIMP__",
        "__MPUX_NEED_MEASUREMENTS"
      ],
      "compilerPath": "/opt/homebrew/bin/riscv64-unknown-elf-gcc",
      "cStandard": "c17",
      "cppStandard": "c++17",
      "intelliSenseMode": "${default}",
      "configurationProvider": "ms-vscode.makefile-tools"
    },
    {
      "name": "binary_distrib",
      "includePath": ["${workspaceFolder}/binary_distrib/bin/ld/*"],
      "defines": [
        "__KDEBUG__",
        "__KERNEL",
        "__mpunix__",
        "__KSIMP__",
        "__KMT__"
      ],
      "compilerPath": "/usr/bin/cc",
      "cStandard": "c17",
      "cppStandard": "c++17",
      "intelliSenseMode": "${default}",
      "configurationProvider": "ms-vscode.makefile-tools"
    }
  ],
  "version": 4
}
